{
	"name": "google_api",
	"properties": {
		"parameters": {
			"key": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{concat('https://maps.googleapis.com/maps/api/geocode/json?key=',linkedService().key)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"key": {
					"type": "AzureKeyVaultSecret",
					"store": {
						"referenceName": "AzureKeyVaultRetta",
						"type": "LinkedServiceReference"
					},
					"secretName": "AccuWeatherAPI"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}