{
	"name": "FinGridAPILoad",
	"properties": {
		"activities": [
			{
				"name": "FirstDayOfPreviousMonth",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "StartDate",
					"value": "@startOfMonth(adddays(startOfMonth(utcnow()),-1))\n"
				}
			},
			{
				"name": "LastDayOfPreviousMonth",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "EndDate",
					"value": "@concat(formatDateTime(adddays(startOfMonth(utcnow()),-1),'yyyy-MM-dd'),'T23:59:00Z')\n"
				}
			},
			{
				"name": "SaveData In TempEnergy_Coeff_Monthly",
				"description": "we are saving data from API to Temporary table s data from API comes for every 3 minutes for the given range",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "FirstDayOfPreviousMonth",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "LastDayOfPreviousMonth",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"enableStaging": false
				}
			}
		],
		"variables": {
			"StartDate": {
				"type": "String"
			},
			"EndDate": {
				"type": "String"
			}
		},
		"folder": {
			"name": "finGrid"
		},
		"annotations": []
	}
}